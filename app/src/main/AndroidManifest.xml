<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.assignment.googlecustomsearch">

    <uses-permission android:name="android.permission.INTERNET"/>
    
    <application
            android:allowBackup="true"
            android:icon="@mipmap/ic_launcher"
            android:label="@string/app_name"
            android:roundIcon="@mipmap/ic_launcher_gcs_round"
            android:supportsRtl="true"
            android:usesCleartextTraffic="true"
            android:theme="@style/AppTheme">
        <activity android:name="com.assignment.googlecustomsearch.activities.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>

        <activity android:name="com.assignment.googlecustomsearch.activities.ListActivity"/>
        <activity android:name="com.assignment.googlecustomsearch.activities.DetailActivity"/>
    </application>

</manifest>